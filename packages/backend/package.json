    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.0",
    "axios": "^1.6.0",
    "express-rate-limit": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
